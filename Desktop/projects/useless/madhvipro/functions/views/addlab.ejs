                              <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                                 <div class="md-form">
                                    <input type="text" id="testlabname" class="form-control" name="testlabname" required>
                                    <label for="testlabname">Test lab name</label>
                                 </div>

                         <div class="md-form">
                                    <input type="text" id="mtestlabname" class="form-control" name="mtestlabname" required>
                                    <label for="mtestlabname">चाचणी प्रयोगशाळा नाव</label>
                                 </div>
                                 <div class="md-form">
                                    <input type="text" id="address" class="form-control" name="address" required>
                                    <label for="address">Test lab address</label>
                                 </div>

                                 <div class="md-form">
                                    <input type="text" id="maddress" class="form-control" name="maddress" required>
